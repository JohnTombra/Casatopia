<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#222222"
    tools:context=".admin_side.activities.AdminHome">



   <androidx.constraintlayout.widget.ConstraintLayout
       android:layout_width="match_parent"
       app:layout_constraintTop_toTopOf="parent"
       android:layout_height="wrap_content">

      <androidx.appcompat.widget.AppCompatImageView
          android:layout_width="match_parent"
          android:background="#222222"
          app:layout_constraintTop_toTopOf="parent"
          android:layout_height="60.5dp"/>

      <androidx.cardview.widget.CardView
          android:layout_width="match_parent"
          android:layout_height="100dp"
          app:cardBackgroundColor="#222222"
          app:layout_constraintTop_toTopOf="parent">
         <androidx.constraintlayout.widget.ConstraintLayout
             android:layout_width="match_parent"
             android:layout_height="match_parent">


            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="30dp"
                android:layout_marginLeft="10dp"
                android:id="@+id/logo"
                android:background="@drawable/baseline_other_houses_24"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_height="30dp"/>


            <TextView
                app:layout_constraintLeft_toRightOf="@+id/logo"
                app:layout_constraintTop_toTopOf="@+id/logo"
                app:layout_constraintBottom_toBottomOf="@+id/logo"
                android:layout_width="wrap_content"
                android:layout_marginLeft="5dp"
                android:textColor="#222222"
                android:fontFamily="sans-serif"
                android:textStyle="bold"
                android:textSize="17dp"
                android:layout_height="wrap_content"
                android:text="Casatopia"/>


            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="30dp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginRight="10dp"
                android:background="@drawable/baseline_dashboard_24"
                android:layout_height="30dp"/>

            <TextView
                android:textColor="#666666"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@+id/logo"
                app:layout_constraintBottom_toBottomOf="@+id/logo"
                android:layout_width="wrap_content"
                android:textSize="11dp"
                android:layout_marginRight="50dp"
                android:layout_height="wrap_content"
                android:text="Dashboard"/>

         </androidx.constraintlayout.widget.ConstraintLayout>


      </androidx.cardview.widget.CardView>

   </androidx.constraintlayout.widget.ConstraintLayout>





   <androidx.constraintlayout.widget.ConstraintLayout
       android:layout_width="match_parent"
       android:layout_marginTop="90dp"
       android:layout_height="match_parent">


      <androidx.appcompat.widget.LinearLayoutCompat
          android:layout_width="match_parent"
          android:orientation="vertical"
          android:layout_height="match_parent">



         <androidx.cardview.widget.CardView
             android:layout_width="match_parent"
             android:layout_margin="8dp"
             app:cardCornerRadius="10dp"
             app:cardElevation="-1dp"
             app:cardBackgroundColor="#c6c6c6"
             android:layout_height="240dp"
             >

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                app:cardBackgroundColor="#ffffff"
                app:cardCornerRadius="10dp"
                android:elevation="-1dp"
                android:layout_margin="0.5dp"
                app:cardElevation="-1dp"
                android:layout_height="match_parent">

               <androidx.constraintlayout.widget.ConstraintLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">

                  <androidx.cardview.widget.CardView
                      android:layout_width="match_parent"
                      app:cardCornerRadius="8dp"
                      android:elevation="-1dp"
                      android:layout_marginRight="10dp"
                      android:id="@+id/imageCont"
                      android:layout_marginLeft="10dp"
                      app:layout_constraintTop_toTopOf="parent"
                      android:layout_marginTop="12dp"
                      app:cardElevation="-1dp"
                      android:layout_height="wrap_content">

                  <androidx.appcompat.widget.AppCompatImageView
                      android:layout_width="match_parent"
                      android:background="#222222"
                      android:id="@+id/image"
                      android:scaleType="center"
                      android:src="@drawable/ship"
                      android:layout_height="150dp"/>

                  </androidx.cardview.widget.CardView>
                  <TextView
                      android:layout_width="wrap_content"
                      android:textStyle="bold"
                      android:layout_marginTop="15dp"
                      android:textSize="16dp"
                      android:textColor="#222222"
                      android:layout_height="wrap_content"
                      android:text="Great White Double Height House"
                      app:layout_constraintLeft_toLeftOf="@id/imageCont"
                      app:layout_constraintTop_toBottomOf="@+id/imageCont"
                      android:id="@+id/name"/>


                  <TextView
                      android:layout_width="wrap_content"
                      android:textStyle="bold"
                      android:layout_marginTop="15dp"
                      android:textSize="15dp"
                      android:layout_height="wrap_content"
                      android:text="N 712,000"
                      android:textColor="#00B0FF"
                      app:layout_constraintRight_toRightOf="@id/imageCont"
                      app:layout_constraintTop_toBottomOf="@+id/imageCont"
                      android:id="@+id/price"/>


                  <androidx.cardview.widget.CardView
                      android:layout_width="wrap_content"
                      android:layout_marginTop="10dp"
                      app:cardBackgroundColor="#FFC107"
                      app:cardCornerRadius="10dp"
                      android:visibility="gone"
                      app:cardElevation="-1dp"
                      app:layout_constraintRight_toRightOf="@+id/price"
                      app:layout_constraintBottom_toBottomOf="@+id/address"
                      app:layout_constraintTop_toBottomOf="@+id/price"
                      android:layout_height="wrap_content">

                     <TextView
                         android:layout_width="50dp"
                         android:textSize="10dp"
                         android:layout_height="20dp"
                         android:gravity="center"
                         android:text="Rent"
                         android:textStyle="bold"
                         android:textColor="#222222"
                        android:id="@+id/type"/>

                  </androidx.cardview.widget.CardView>

                  <TextView
                      android:layout_width="wrap_content"
                      android:layout_marginTop="10dp"
                      android:textSize="11dp"
                      android:layout_height="wrap_content"
                      android:text="3891 Ranchview Dr.Richardson, California"
                      app:layout_constraintLeft_toLeftOf="@id/name"
                      app:layout_constraintTop_toBottomOf="@+id/name"
                      android:id="@+id/address"/>


               </androidx.constraintlayout.widget.ConstraintLayout>


            </androidx.cardview.widget.CardView>

         </androidx.cardview.widget.CardView>





         <androidx.cardview.widget.CardView
             android:layout_width="match_parent"
             android:layout_marginLeft="8dp"
             android:layout_marginRight="8dp"
             android:layout_marginBottom="8dp"
             android:layout_marginTop="15dp"
             app:cardCornerRadius="10dp"
             app:cardElevation="-1dp"
             app:cardBackgroundColor="#c6c6c6"
             android:layout_height="240dp"
             >

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                app:cardBackgroundColor="#ffffff"
                app:cardCornerRadius="10dp"
                android:elevation="-1dp"
                app:cardElevation="-1dp"
                android:layout_margin="0.5dp"
                android:layout_height="match_parent">

               <androidx.constraintlayout.widget.ConstraintLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">

                  <androidx.cardview.widget.CardView
                      android:layout_width="match_parent"
                      app:cardCornerRadius="8dp"
                      android:elevation="-1dp"
                      android:layout_marginRight="10dp"
                      android:id="@+id/imageCont2"
                      android:layout_marginLeft="10dp"
                      app:layout_constraintTop_toTopOf="parent"
                      android:layout_marginTop="12dp"
                      app:cardElevation="-1dp"
                      android:layout_height="wrap_content">

                     <androidx.appcompat.widget.AppCompatImageView
                         android:layout_width="match_parent"
                         android:background="#c6c6c6"
                         android:id="@+id/image2"
                         android:scaleType="center"
                         android:src="@drawable/ship2"
                         android:layout_height="150dp"/>

                  </androidx.cardview.widget.CardView>
                  <TextView
                      android:layout_width="wrap_content"
                      android:textStyle="bold"
                      android:textColor="#222222"
                      android:layout_marginTop="15dp"
                      android:textSize="16dp"
                      android:layout_height="wrap_content"
                      android:text="Great White Double Height House"
                      app:layout_constraintLeft_toLeftOf="@id/imageCont2"
                      app:layout_constraintTop_toBottomOf="@+id/imageCont2"
                      android:id="@+id/name2"/>


                  <TextView
                      android:layout_width="wrap_content"
                      android:textStyle="bold"
                      android:layout_marginTop="15dp"
                      android:textSize="15dp"
                      android:layout_height="wrap_content"
                      android:text="N712,250"
                      android:textColor="#00B0FF"
                      app:layout_constraintRight_toRightOf="@id/imageCont2"
                      app:layout_constraintTop_toBottomOf="@+id/imageCont2"
                      android:id="@+id/price2"/>


                  <androidx.cardview.widget.CardView
                      android:layout_width="wrap_content"
                      android:layout_marginTop="10dp"
                      app:cardBackgroundColor="#FFC107"
                      app:cardCornerRadius="10dp"
                      android:visibility="gone"
                      app:cardElevation="-1dp"
                      app:layout_constraintRight_toRightOf="@+id/price2"
                      app:layout_constraintBottom_toBottomOf="@+id/address2"
                      app:layout_constraintTop_toBottomOf="@+id/price2"
                      android:layout_height="wrap_content">

                     <TextView
                         android:layout_width="50dp"
                         android:textSize="10dp"
                         android:layout_height="20dp"
                         android:gravity="center"
                         android:text="Rent"
                         android:textStyle="bold"
                         android:textColor="#222222"
                         android:id="@+id/type2"/>

                  </androidx.cardview.widget.CardView>

                  <TextView
                      android:layout_width="wrap_content"
                      android:layout_marginTop="10dp"
                      android:textSize="11dp"
                      android:layout_height="wrap_content"
                      android:text="3891 Ranchview Dr.Richardson, California"
                      app:layout_constraintLeft_toLeftOf="@id/name2"
                      app:layout_constraintTop_toBottomOf="@+id/name2"
                      android:id="@+id/address2"/>


               </androidx.constraintlayout.widget.ConstraintLayout>


            </androidx.cardview.widget.CardView>

         </androidx.cardview.widget.CardView>






         <androidx.cardview.widget.CardView
             android:layout_width="match_parent"
             android:layout_marginLeft="8dp"
             android:layout_marginRight="8dp"
             android:layout_marginBottom="8dp"
             android:layout_marginTop="15dp"
             app:cardCornerRadius="15dp"
             app:cardElevation="-1dp"
             app:cardBackgroundColor="#c6c6c6"
             android:layout_height="240dp"
             >

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                app:cardBackgroundColor="#ffffff"
                app:cardCornerRadius="10dp"
                android:elevation="-1dp"
                android:layout_margin="0.5dp"
                app:cardElevation="-1dp"
                android:layout_height="match_parent">

               <androidx.constraintlayout.widget.ConstraintLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">

                  <androidx.cardview.widget.CardView
                      android:layout_width="match_parent"
                      app:cardCornerRadius="10dp"
                      android:elevation="-1dp"
                      android:layout_marginRight="10dp"
                      android:id="@+id/imageCont3"
                      android:layout_marginLeft="10dp"
                      app:layout_constraintTop_toTopOf="parent"
                      android:layout_marginTop="12dp"
                      app:cardElevation="-1dp"
                      android:layout_height="wrap_content">

                     <androidx.appcompat.widget.AppCompatImageView
                         android:layout_width="match_parent"
                         android:background="#c6c6c6"
                         android:id="@+id/image3"
                         android:textSize="13dp"
                         android:scaleType="center"
                         android:src="@drawable/ship3"
                         android:layout_height="160dp"/>

                  </androidx.cardview.widget.CardView>
                  <TextView
                      android:layout_width="wrap_content"
                      android:textStyle="bold"
                      android:layout_marginTop="10dp"
                      android:textSize="16dp"
                      android:layout_height="wrap_content"
                      android:text="Great White Double Height House"
                      app:layout_constraintLeft_toLeftOf="@id/imageCont3"
                      app:layout_constraintTop_toBottomOf="@+id/imageCont3"
                      android:id="@+id/name3"/>


                  <TextView
                      android:layout_width="wrap_content"
                      android:textStyle="bold"
                      android:layout_marginTop="10dp"
                      android:textSize="16dp"
                      android:layout_height="wrap_content"
                      android:text="N250"
                      android:textColor="#39B9E3"
                      app:layout_constraintRight_toRightOf="@id/imageCont3"
                      app:layout_constraintTop_toBottomOf="@+id/imageCont3"
                      android:id="@+id/price3"/>


                  <androidx.cardview.widget.CardView
                      android:layout_width="wrap_content"
                      android:layout_marginTop="10dp"
                      app:cardBackgroundColor="#FFC107"
                      app:cardCornerRadius="10dp"
                      android:visibility="gone"
                      app:cardElevation="-1dp"
                      app:layout_constraintRight_toRightOf="@+id/price3"
                      app:layout_constraintBottom_toBottomOf="@+id/address3"
                      app:layout_constraintTop_toBottomOf="@+id/price3"
                      android:layout_height="wrap_content">

                     <TextView
                         android:layout_width="50dp"
                         android:textSize="10dp"
                         android:layout_height="20dp"
                         android:gravity="center"
                         android:text="Rent"
                         android:textStyle="bold"
                         android:textColor="#222222"
                         android:id="@+id/type3"/>

                  </androidx.cardview.widget.CardView>

                  <TextView
                      android:layout_width="wrap_content"
                      android:layout_marginTop="10dp"
                      android:textSize="13dp"
                      android:layout_height="wrap_content"
                      android:text="3891 Ranchview Dr.Richardson, California"
                      app:layout_constraintLeft_toLeftOf="@id/name3"
                      app:layout_constraintTop_toBottomOf="@+id/name3"
                      android:id="@+id/address3"/>


               </androidx.constraintlayout.widget.ConstraintLayout>


            </androidx.cardview.widget.CardView>

         </androidx.cardview.widget.CardView>



      </androidx.appcompat.widget.LinearLayoutCompat>


   </androidx.constraintlayout.widget.ConstraintLayout>





   <androidx.constraintlayout.widget.ConstraintLayout
       android:layout_width="match_parent"
       app:layout_constraintBottom_toBottomOf="parent"
       android:layout_height="wrap_content">

      <ImageView
          android:background="#c6c6c6"
          app:layout_constraintTop_toTopOf="parent"
          android:layout_width="match_parent"
          android:layout_height="50.5dp"/>

      <androidx.cardview.widget.CardView
          app:cardElevation="100dp"
          app:layout_constraintBottom_toBottomOf="parent"
          android:layout_width="match_parent"
          android:layout_height="50dp">


         <LinearLayout
             android:layout_width="match_parent"
             android:orientation="horizontal"
             android:layout_gravity="bottom"
             android:layout_height="50dp">



            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="0dp"
               android:layout_weight="1"
                android:orientation="vertical"
                android:layout_height="match_parent">
               <TextView
                   android:layout_width="match_parent"
                   android:text="Properties"
                   android:gravity="center"
                   android:textSize="11dp"
                   android:textColor="#666666"
                   android:layout_height="0dp"
                   android:layout_weight="1"/>

               <ImageView
                   android:layout_width="match_parent"
                   android:layout_height="3dp"
                   android:background="#00bbff"/>
            </androidx.appcompat.widget.LinearLayoutCompat>





            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="0dp"
                android:layout_weight="1"
                android:orientation="vertical"
                android:layout_height="match_parent">
               <TextView
                   android:layout_width="match_parent"
                   android:text="Clients"
                   android:textSize="11dp"
                   android:gravity="center"
                   android:textColor="#666666"
                   android:layout_height="0dp"
                   android:layout_weight="1"/>

               <ImageView
                   android:layout_width="match_parent"
                   android:layout_height="3dp"
                   android:visibility="gone"
                   android:background="#00bbff"/>
            </androidx.appcompat.widget.LinearLayoutCompat>




            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="0dp"
                android:layout_weight="1"
                android:orientation="vertical"
                android:layout_height="match_parent">
               <TextView
                   android:layout_width="match_parent"
                   android:text="Chats"
                   android:gravity="center"
                   android:textSize="11dp"
                   android:textColor="#666666"
                   android:layout_height="0dp"
                   android:layout_weight="1"/>

               <ImageView
                   android:layout_width="match_parent"
                   android:layout_height="3dp"
                   android:visibility="gone"
                   android:background="#00bbff"/>
            </androidx.appcompat.widget.LinearLayoutCompat>



            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="0dp"
                android:layout_weight="1"
                android:orientation="vertical"
                android:layout_height="match_parent">
               <TextView
                   android:layout_width="match_parent"
                   android:text="Transactions"
                   android:gravity="center"
                   android:textSize="11dp"
                   android:layout_height="0dp"
                   android:textColor="#666666"
                   android:layout_weight="1"/>

               <ImageView
                   android:layout_width="match_parent"
                   android:layout_height="4dp"
                   android:visibility="gone"
                   android:background="#00bbff"/>
            </androidx.appcompat.widget.LinearLayoutCompat>



         </LinearLayout>

      </androidx.cardview.widget.CardView>

   </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>